@model List<Application.ViewModels.SeriesViewModel.SeriesViewModel>

<div class="row">

    @if (Model.Count == 0)
    {
        <h2> MANITO ESTA DATABASE ESTA VACIA</h2>
    }
    else
    {   
        @foreach(Application.ViewModels.SeriesViewModel.SeriesViewModel item in Model)
        {

             <div class="col-4">
                <div class="card shadow - sn">
                    <img  class="bd-placeholder-img card-img-top" src="@item.ImagenUrl">

                    <div class="card-body">
                        <h4>@item.</h4>
                        <p class="card text"> @item.VideoUrl</p>

                    </div>

                </div>

             </div>
        }
    }
</div>